{
    "title": "Rcommand to copy & Roption to paste",
    "rules": [
      {
        "description": "map l_command+eject to scroll up",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "consumer_key_code": "eject",
              "modifiers": {
                "mandatory": [
                  "left_command"
                ]
              }
            },
            "to": [
              {
                "mouse_key": {
                  "vertical_wheel": -32
                }
              }
            ]
          }
        ]
      },
      {
        "description": "map eject to scroll down",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "consumer_key_code": "eject"
            },
            "to": [
              {
                "mouse_key": {
                  "vertical_wheel": 32
                }
              }
            ]
          }
        ]
      },
      {
        "description": "map fn+f12 to brightnesss up",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "f12",
              "modifiers": {
                "mandatory": [
                  "fn"
                ]
              }
            },
            "to": [
              {
                "key_code": "display_brightness_increment"
              }
            ]
          }
        ]
      },
      {
        "description": "map fn+f11 to brightnesss down",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "f11",
              "modifiers": {
                "mandatory": [
                  "fn"
                ]
              }
            },
            "to": [
              {
                "key_code": "display_brightness_decrement"
              }
            ]
          }
        ]
      },
      {
        "description": "map f1 (magic keyboard) to esc",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "f4",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "spacebar",
                "modifiers": [
                  "left_control"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "Change l_option+c to command+d then command+c in VSCODE",
        "manipulators": [
          {
            "conditions": [
              {
                "bundle_identifiers": [
                  "^com.microsoft.VSCode$"
                ],
                "type": "frontmost_application_if"
              }
            ],
            "type": "basic",
            "from": {
              "key_code": "c",
              "modifiers": {
                "mandatory": [
                  "left_option"
                ]
              }
            },
            "to": [
              {
                "key_code": "d",
                "modifiers": [
                  "left_command"
                ]
              },
              {
                "key_code": "c",
                "modifiers": [
                  "left_command"
                ]
              }
            ]
          }
        ]
      },
      {
        "description": "Change l_shift+spacebar to f17",
